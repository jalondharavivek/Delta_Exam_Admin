<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Category</title>
    <link rel="stylesheet" href="/css/result.css">

</head>

<body>
    <%- include('header'); -%>
    <div class="main-container">
        <%- include('nav'); -%>
            <div class="main">
    <h1>student name</h1>
    <table class="table table-hover">
       
        <thead>
            <tr>
                <th>No</th>
                <th>student name</th>
                <th>status</th>
            </tr>
        </thead>
        <tbody id="tbody">
            <%for(var i=0;i<data.length;i++){%>
                <tr>
                    <td>
                        <%=i+1%>
                    </td>
                    <td>
                        <%=data[i].name%>
                    </td>
                    <td><a href="/companylist?id=<%= data[i].user_id %>" id="<%= data[i].user_id %>">View</a></td>
                </tr>
                <%}%>
        </tbody>
        

       
    </table>

                <div id="page">
                    <p onclick="page(`1`,`<%=count%>`)" class="p">prev</p>
                    <%for(i=1 ;i<=count ;i++){%>
                        <% if(i==1){ %>
                        <p onclick="page(`<%=i%>`,`<%=count%>`)" class="p">
                            <b><%=i%></b>
                        </p>
                        <% }else{ %>
                        <p onclick="page(`<%=i%>`,`<%=count%>`)" class="p">
                            <%=i%>
                        </p>

                    <% } %>
                    <% } %>
                    <p onclick="page(`2`,`<%=count%>`)" class="p">next</p>
                </div>
            </div>
            </div>
            
</body>
<script src="/js/result.js"></script>
<script src="/js/dashboard.js"></script>

</html>