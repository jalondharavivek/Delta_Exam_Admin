<body>
    <%- include('header'); -%>
    <div class="main-container">
        <%- include('nav'); -%>
        <div class="main">
            <div class="add-link">
                <div class="searchbar">
                    <input type="text" name="" id="" placeholder="Search">
                    <div class="searchbtn">
                        <img src="https://media.geeksforgeeks.org/wp-content/uploads/20221210180758/Untitled-design-(28).png" class="icn srchicn" alt="search-button">
                    </div>
                </div>
              
            </div>
            <table>
               
                <tr>
                    <th>Category name</th>
                    <th>view question</th>
                </tr>   
            <% let id=1; %>
            <% data.forEach(c => { %>
                <tr>
                      <td><%= c.category_name %></td>
                     <td><a href="/viewquestionresult" class="btn btn-dark"><i class="fa-solid fa-circle-info"></i></a></td>
                        
                </tr>
                <% }); %>
        </table>

            
        </div>
    </div>
</body>

</html>