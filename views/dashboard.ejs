<body>

    <!-- for header part -->
    <%- include('header'); -%>

  

    <div class="main-container">
        <%- include('nav'); -%>
     
        <div class="main">

            
           
            <div class="box-container">
                <div class="box box1">
                    <a href="category">
                    <div class="text">
