<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <title>Result</title>
</head>
<body>
    <%var count=0 %>
    <%for(var i=0;i<data.length;i++){%>
        <% text1=data[i].answer %>
        <% text2=data[i].user_answers %>
        <%if(text1.localeCompare(text2) !=0 ){%>
            <% count++ %>
            <% } %>
    <%}%>
    <p>total Mark:<%=data.length%></p><br>
    <p>achived Mark:<%=data.length-count%></p>
    <table class="table table-hover">
        <tr>
            <td>No</td>
            <td>Question </td>
            <td> answer</td>
            <td>Student answer</td>
        </tr>
        <%for(var i=1;i<=data.length;i++){%>
            <tr>
            <td><%=i%></td>
            <td><%=data[i-1].question_text%></td>
            <td><%=data[i-1].answer%></td>
            <td><%=data[i-1].user_answers%></td>
            </tr>
        <%}%>
    </table>
</body>
 <script src="/js/result.js"></script>
 <script src="/js/dashboard.js"></script>
</html>

